{
  "manifest_version": 2,

  "name": "Kontent Script",
  "description": "Experimental content scripts",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "km-sq-1.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
